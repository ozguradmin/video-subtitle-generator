{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 300
    }
  },
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  }
}
